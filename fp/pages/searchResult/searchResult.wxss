/* pages/searchResult/searchResult.wxss */

.search-result {
}

.search-box {
  padding: 10rpx 30rpx;
  background-color: white;
  border-top: 1rpx solid #f8f8f8;
  border-bottom: 1rpx solid #f8f8f8;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.search-box .search-input {
  display: flex;
  flex-direction: row;
  align-items: center;
  background: rgba(249, 249, 249, 1);
  border-radius: 30rpx;
}

.search-box .left-icon {
  width: 31rpx;
  height: 30rpx;
}

.product-item {
  padding: 30rpx;
  background-color: white;
  border-bottom: 1rpx solid #f8f8f8;
}
