/* 分类 s*/

.index .module-category {
  display: flex;
  flex-direction: row;
  background-color: white;
  margin-bottom: 10rpx;
}

.module-category .module-category__item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-right: 1rpx solid #d1d1d1;
  margin: 30rpx 0;
}

.module-category .module-category__item.border--null {
  border: none;
}

.module-category__item .module-category__item--name {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 28rpx;
  margin-bottom: 10rpx;
}

.module-category__item .module-category__item--name.module-category__active {
  color: rgba(254, 142, 24, 1);
}

.module-category__item .module-category__item--title.module-category__active {
  background: linear-gradient(90deg, rgba(255, 182, 33, 1) 0%, rgba(254, 137, 23, 1) 100%);
  color: white;
}

.module-category__item .module-category__item--title {
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
  padding: 10rpx;
  border-radius: 30rpx;
}

/* 分类 e*/

/* 分类每一项s */

.index .category-item {
  display: none;
}

.index .category-item.category--active {
  display: block;
}

/* 分类每一项e */

/* 臻品馆s */

.category-item .today-high__sell .category-item__title--img {
  margin: 0 auto;
  width: 328rpx;
  height: 60rpx;
}

/* 今日爆款S */

.category-item .today-high__sell {
  position: relative;
  background-color: white;
  padding: 11rpx 0 0rpx;
  margin-bottom: 10rpx;
}

.today-high__sell .today-high__sell--bottom {
}

.today-high__sell .today-high__sell--bottom {
  white-space: nowrap;
  /* overflow: hidden; */
}

.today-high__sell .today-high__sell--item {
  position: relative;
  display: inline-block;
  margin-right: 10rpx;
  border: 1px solid #f8f8f8;
  width: 240rpx;
  margin-top: 20rpx;
  margin-bottom: 23rpx;
}

.today-high__sell .today-high__sell--item .product-box {
}

.today-high__sell .today-high__sell--item.right--30rpx {
  margin-left: 30rpx;
}

.today-high__sell .product-photo {
  width: 240rpx;
  height: 208rpx;
}

.product-box .free-freight__icon {
  width: 50rpx;
  height: 28rpx;
  margin-right: 10rpx;
}

.product-box .product-discription {
  white-space: pre-wrap;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 30rpx;
  padding: 0 25rpx 31rpx 25rpx;
}

.product-box .people-count {
  font-size: 20rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 24rpx;
  padding: 0 0 24rpx 25rpx;
}

.product-box .product-price {
  font-size: 22rpx;
  font-weight: 400;
  color: rgba(255, 166, 81, 1);
  line-height: 24rpx;
  padding: 0 0 24rpx 24rpx;
}

.today-high__sell--item .product-rob__icon {
  position: absolute;
  width: 42rpx;
  height: 41rpx;
  bottom: 25rpx;
  right: 20rpx;
}

.today-high__sell--item .hot-icon {
  position: absolute;
  width: 36rpx;
  height: 43rpx;
  left: 5rpx;
  top: -20rpx;
}

