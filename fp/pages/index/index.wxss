/* 特色好店s */

.characteristic-store__item {
  padding: 30rpx 25rpx;
  border-bottom: 1rpx solid #f8f8f8;
  background-color: white;
}

.characteristic-store__item .top {
  display: flex;
  flex-direction: row;
}

.characteristic-store__item .top .store__avatar {
  width: 64rpx;
  height: 64rpx;
  margin-right: 17rpx;
}

.characteristic-store__item .top .store__name {
  flex: 1;
  display: flex;
  flex-direction: column;
}

.characteristic-store__item .store__name .store-name__top {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 36rpx;
}

.characteristic-store__item .store__name .store-name__bottom {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 36rpx;
}

.characteristic-store__item .top .margin-right--30rpx {
  margin-left: 30rpx;
}

.characteristic-store__item .top .join-store {
  width: 140rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  background: linear-gradient(270deg, rgba(254, 139, 23, 1) 0%, rgba(255, 181, 33, 1) 100%);
  border-radius: 25rpx;
  font-size: 26rpx;
  color: rgba(255, 255, 255, 1);
  margin-left: 30rpx;
}

.characteristic-store__item .bottom {
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
}

.characteristic-store__item .bottom .store-product__item {
  position: relative;
  width: 222rpx;
  display: flex;
  flex-direction: column;
  margin-right: 16rpx;
}

.bottom .store-product__item .store-product__photo {
  height: 222rpx;
  width: 222rpx;
  border-radius: 6rpx;
}

.bottom .store-product__item .product-price {
  position: absolute;
  bottom: 10rpx;
  right: 0rpx;
  height: 30rpx;
  width: 72rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 22rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  background: rgba(255, 255, 255, 1);
  opacity: 0.5;
  border-radius: 0px 0px 6rpx 0px;
}

