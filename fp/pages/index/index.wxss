/* 头部搜索s */

.index .hand-search {
  display: flex;
  flex-direction: row;
  position: absolute;
  z-index: 1;
  margin-top: 17rpx;
}

.hand-search .left {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 30rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  margin-left: 30rpx;
}

.hand-search .left image {
  margin-left: 10rpx;
}

.hand-search .left .hand-search__biao {
  width: 13rpx;
  height: 7rpx;
}

.hand-search .middle {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 405rpx;
  padding-left: 80rpx;
  height: 56rpx;
  background: rgba(255, 255, 255, 1);
  opacity: 0.65;
  border-radius: 28rpx;
  margin: 0 48rpx 0 32rpx;
}

.hand-search .middle .hand-search__jing {
  width: 33rpx;
  height: 31rpx;
  margin-right: 20rpx;
}

.hand-search .middle .hand-search__input {
  font-size: 22rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.hand-search .middle .hand-search__input--text {
  font-size: 22rpx;
  font-weight: 400;
}

.hand-search .hand-search__other {
  width: 44rpx;
  height: 44rpx;
}

