/* pages/LiaisonStore/LiaisonStore.wxss */

.container {
  padding: 30rpx;
}

.liaison-store .photo {
  width: 690rpx;
  height: 243rpx;
}

.liaison-store .box {
  background-color: white;
  padding-bottom: 100rpx;
}

.liaison-store .mobile-number {
  font-size: 30rpx;
  color: rgba(51, 51, 51, 1);
  text-align: center;
  margin: 70rpx 0 90rpx;
}

.liaison-store .btn {
  width: 400rpx;
  height: 88rpx;
  background: linear-gradient(89deg, rgba(253, 179, 32, 1) 0%, rgba(254, 141, 22, 1) 100%);
  border-radius: 44rpx;
  font-size: 30rpx;
  color: rgba(254, 254, 254, 1);
}
